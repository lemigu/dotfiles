{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FFFFFF",
          "style": "plain",
          "template": "\u001b[1m@lemigu\u001b[0m",
          "type": "session"
        },
        {
          "foreground": "#FFFFFF",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} >",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
